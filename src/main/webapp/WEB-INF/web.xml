<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="https://jakarta.ee/xml/ns/jakartaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd"
         version="6.0">
    <servlet>
        <servlet-name>home_page</servlet-name>
        <servlet-class>com.example.ensa_events.home_page</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>home_page</servlet-name>
        <url-pattern>/home_page</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>next_events</servlet-name>
        <servlet-class>com.example.ensa_events.next_events</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>next_events</servlet-name>
        <url-pattern>/next_events</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>admin_panel</servlet-name>
        <servlet-class>com.example.ensa_events.admin_panel</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>admin_panel</servlet-name>
        <url-pattern>/admin_panel</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>add_new_event</servlet-name>
        <servlet-class>com.example.ensa_events.add_new_event</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>add_new_event</servlet-name>
        <url-pattern>/admin_pane/add_new_event</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>add_archive</servlet-name>
        <servlet-class>com.example.ensa_events.add_archive</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>add_archive</servlet-name>
        <url-pattern>/admin_pane/add_archive</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>com.example.ensa_events.register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.example.ensa_events.login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
</web-app>
